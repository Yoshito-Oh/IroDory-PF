<div class="container">
  <div class="row">
    <div class="col-lg-12">
      <% if @pending_images.each do |post_image| %>
      <div class="un_apply_list">
        <h2>未承認作品の一覧</h2>
        <div class="un_apply_image" style="display:inline-block">
          <%= attachment_image_tag post_image, :image , size: "200x200" %>
        </div>
        <div>
          <%= post_image.user.nickname %><!--作者名-->
          <%= post_image.title %><!--作品タイトル-->
          <%= post_image.img_introduction %><!--作品説明-->
        </div>

        </div>
      <% end %>
      </div>
    </div>
  </div>
</div>